doctype html
html
  head
    - var number = 33
    meta(charset="utf-8")
    title 任务#{number}
    link(rel="stylesheet", type="text/css" href="styles.css")
  
  body
    table(border="0", cellspacing="0", cellpadding="0")#table
      tbody
        tr
          td
          -for (var i=1; i<11; i++)
            td= i
        
        -for (var j=1; j<11; j++)
          tr
            td= j
            -for (var k=0; k<10; k++)
              td
    div.inputs
      input(type="text")#input
      button 执行
    script(type="text/javascript", src="scripts.js")
        
          
